<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title-Event Reminder Sites</title>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryui")
    @Scripts.Render("~/bundles/jqueryval")

    <script src="~/Scripts/jquery.validate.js"></script>
    <link href="~/Content/themes/le-frog/jquery-ui.le-frog.css" rel="stylesheet" />
    <link href="~/Content/themes/le-frog/jquery-ui.theme.css" rel="stylesheet" />

    <style type="text/css">
        body {
            padding: 0px;
            margin: 0px;
            font-family: Tahoma;
            font-size: 13px;
        }
    </style>
</head>
<body>
    <table style="width:100%" cellpadding="0" cellspacing="0">
        <tr>
            <td>
                Headrergoes here
                @Html.Partial("_Header")
            </td>
        </tr>
        <tr>
            <td align="right">
                <b>
                    Last Accessed Date & Time:&nbsp;
                </b>@Convert.ToDateTime(Session["LastAcessedDateTime"]).ToString("dddd,dd MM yyyy hh:mm:ss:tt")
            </td>
        </tr>
        <tr>
            <td>
                @RenderBody()
            </td>

        </tr>

        <tr>
            <td>
                @Html.Partial("_Footer")
            </td>
        </tr>
    </table>
    
</body>
</html>
