<!DOCTYPE html>
@using System.Web.Optimization
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title-Event Reminder Sites</title>
   
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryui")
    @Scripts.Render("~/bundles/jqueryval")

    <script src="~/Scripts/jquery.validate.js"></script>
    <link href="~/Content/themes/le-frog/jquery-ui.le-frog.css" rel="stylesheet" />
    <link href="~/Content/themes/le-frog/jquery-ui.theme.css" rel="stylesheet" />

    <link rel="stylesheet" href="http://code.jquery.com/ui/1.12.0/themes/le-frog/jquery-ui.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.js"></script>

    <style type="text/css">
        body{
            padding:0px;
            margin:0px;
            font-family:Tahoma;
            font-size:13px;
        }

    </style>
</head>
<body>
    
    @Html.Partial("_Header")
       

    <div style="border:2px solid gray">
        @RenderBody()
        </div>
   
    @Html.Partial("_Footer")
</body>
</html>
